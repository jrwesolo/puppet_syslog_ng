    flags(<%= @flags %>)
<%- end -%>
<%- unless @flush_lines.nil? -%>
    flush-lines(<%= @flush_lines %>)
<%- end -%>
<%- unless @localip.nil? -%>
    localip(<%= @localip %>)
<%- end -%>
<%- unless @localport.nil? -%>
    localport(<%= @localport %>)
<%- end -%>
<%- unless @log_fifo_size.nil? -%>
    log-fifo-size(<%= @log_fifo_size %>)
<%- end -%>
<%- if @port -%>
    port(<%= @port %>)
<%- elsif @parsed_host['port'] -%>
    port(<%= @parsed_host['port'] %>)
<%- end -%>
<%- unless @suppress.nil? -%>
    suppress(<%= @suppress %>)
<%- end -%>
